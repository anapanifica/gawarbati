    <TIER LANG_REF="en" LINGUISTIC_TYPE_REF="phrase-item" PARENT_REF="AbdulHakim-txt-gwt-fonipa-x-emic" PARTICIPANT="AbdulHakim" TIER_ID="AbdulHakim_translation-gls-en">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a135" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>The fact is that the two youth have come here</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a136" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>Mr. Naseer and Mr. Abdullah</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a137" ANNOTATION_REF="a85">
                <ANNOTATION_VALUE>They asked me to describe any ashtonishing story of my life, if I have any. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a138" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>An incident that took place in Akrawi.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a139" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>We had a quarrel with the brother of Mula Rustam</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a140" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>It happened that</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a141" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>We were inside the room during this fight.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a142" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>Inside the room, a boy named Abbas</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a143" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>He called out in Nuristani language to kill them with Kalashnikov</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a144" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>By uniting with me</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a145" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>During this time I came forward and God saved us. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a146" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>Allah[Lord] saved him. Man had no ability in it</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a147" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE>I pulled out the barrel of the gun and Ghulam reached instantly. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a148" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE>Ghulam Lala hit Markor with the butt of a gun and hit him in such a way that </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a149" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE>He had a big knife and I pulled out the barrel of Kalashnikov of the gun and threw it away. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a150" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>After throwing, I cut my finger with this knife in such a way that it reaches to the bone.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a151" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE>The way he came and beat me in such a way hahaha [Laugh]</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a152" ANNOTATION_REF="a25">
                <ANNOTATION_VALUE>The situation reached to this this point that</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a153" ANNOTATION_REF="a26">
                <ANNOTATION_VALUE>The guns of these people were sold and they were also released from jail.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a154" ANNOTATION_REF="a27">
                <ANNOTATION_VALUE>This was a story of my life</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a155" ANNOTATION_REF="a28">
                <ANNOTATION_VALUE>It is weired</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a156" ANNOTATION_REF="a29">
                <ANNOTATION_VALUE>Besides, who in my nation sees or hears this?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a157" ANNOTATION_REF="a30">
                <ANNOTATION_VALUE>My request to them is that Allah[Lord] has made hard work a condition for human beings.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
